{
  "name": "fwd.place",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "prisma": "dotenv -e .env.local -- prisma",
    "prisma:seed": "esbuild prisma/seed.ts --bundle --format=cjs --external:prisma --external:@prisma/client --outfile=prisma/build/seed.cjs && dotenv -e .env.local -- node prisma/build/seed.cjs",
    "codegen": "graphql-codegen --config codegen.cjs",
    "postinstall": "npm run codegen",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write ."
  },
  "devDependencies": {
    "@envelop/graphql-jit": "^5.0.4",
    "@graphql-codegen/cli": "3.0.0",
    "@graphql-codegen/introspection": "3.0.0",
    "@graphql-codegen/typescript": "3.0.0",
    "@graphql-codegen/typescript-document-nodes": "3.0.0",
    "@graphql-tools/utils": "^9.2.0",
    "@prisma/client": "4.9.0",
    "@rollup/plugin-graphql": "^2.0.3",
    "@sveltejs/adapter-auto": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "codegen": "^0.1.0",
    "daisyui": "^2.50.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "graphql": "^16.6.0",
    "graphql-scalars": "^1.20.1",
    "graphql-tag": "^2.12.6",
    "graphql-yoga": "^3.5.1",
    "postcss": "^8.4.21",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "prisma": "^4.9.0",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.5",
    "vite": "^4.0.0",
    "vitest": "^0.25.3"
  },
  "type": "module"
}
